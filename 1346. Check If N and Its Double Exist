class Solution {
    public boolean checkIfExist(int[] arr) {
        HashSet<Integer> set = new HashSet<>();
        
        for (int num : arr) {

            if (set.contains(num * 2) || (num % 2 == 0 && set.contains(num / 2))) {      #We check if the num*2 is present in set or not if present then return true 
                return true;
            }
            set.add(num);       #we add the num into set
        }
        
        return false;
    }
}  
